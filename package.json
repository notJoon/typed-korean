{
  "name": "typed-korean",
  "version": "0.0.1",
  "description": "Type-level Korean grammar validation using TypeScript's type system",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "codegen": "tsx scripts/gen-jamo-table.ts",
    "test": "vitest --typecheck --passWithNoTests",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "3.2.4"
  },
  "license": "MIT"
}
